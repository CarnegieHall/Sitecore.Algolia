<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <contentSearch>
      <configuration type="Sitecore.ContentSearch.ContentSearchConfiguration, Sitecore.ContentSearch">
        <indexes hint="list:AddIndex">
          <index id="algolia_master_index" type="Algolia.SitecoreProvider.AlgoliaSearchIndex, Algolia.SitecoreProvider">
            <param desc="name">$(id)</param>
            <param desc="applicationId">3Q92VD0BCR</param>
            <param desc="fullApiKey">ae8283da0a1342b03749ed1355d9d4a7</param>
            <param desc="indexName">sitecore</param>
            <param desc="propertyStore" ref="contentSearch/databasePropertyStore" param1="$(id)" />
            <strategies hint="list:AddStrategy">
              <!-- NOTE: order of these is controls the execution order -->
              <strategy ref="contentSearch/indexUpdateStrategies/syncMaster" />
            </strategies>
            <locations hint="list:AddCrawler">
              <crawler type="Algolia.SitecoreProvider.AlgoliaCrawler, Algolia.SitecoreProvider">
                <Database>master</Database>
                <Root>/sitecore</Root>
                <Operations type="Algolia.SitecoreProvider.AlgoliaIndexOperations, Algolia.SitecoreProvider" />
              </crawler>
            </locations>
          </index>
        </indexes>
      </configuration>
    </contentSearch>
  </sitecore>
</configuration>